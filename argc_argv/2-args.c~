#include "main.h"

/**
 *main - the name of the program
 *@argc: the number of arguments passed to string
 *@argv: the argument vector of a pointer
 *
 *Return: Always 0
 */

int main(int argc, char *argv[])
{
	int i;

	for (i = 0; i < argc; i++)
	{
		printf("%s\n", argv[i]);
	}
	printf("\n");
	return (0);
}
